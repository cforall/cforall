struct S {
	int i;
};

struct T {
	S s;
};

int main() {
	int j = 0;
	S s = { 0 };
	T t = { { 0 } };

	j = j;
	s = s;
	s.i = s.i;
	t.s.i = t.s.i;

	#pragma message( "Compiled" )			// force non-empty .expect file
}

