#include <stdio.h>

enum(int) IntEnum {
    zero,
    one,
    thousand = 1000,
    thousand_one,
    two_thousand = 2000,
    two_thousand_one,
    two_thousand_two
};

int main() {
    printf("0=%d\n", zero);
