int f( int i ) { return i; }

int main( void ) {
	int i = 0;
	switch ( i ) case 3 : i = 1;
	switch ( i ) default : f( 3 );

	switch ( 3 )
	  default:
	  case 2:
	  case 3:
		f( 3 );

	switch ( i ) {}
	switch ( i ) {
	  case 3:
		f( 3 );
	} // switch

	switch ( 3 ) {
		int j;
	  case 3:
		break;
	  case 4:
		j = 0;
	}

	switch ( i ) {
	  case 1, 2, 3:
		switch ( i ) {
		  case 2, 3, 4:
			7;
		}
	}

	switch ( i ) {
		int j = 0;
		int k = 0;
		struct S { int i; };
		S s;
	  case 8~10:
	  default:
		i = 3;
	  case 19:
	  case 'A' ... 'Z':
	  case 1 ... 6:
	  case 20, 30:
		j = 3;
		f( 3 );
		break;
	} // switch

	choose ( i ) case 3 : f( 3 );
	choose ( i ) default : i = 1;

	choose ( 3 )
	  case 2:
	  default:
	  case 3:
		f( 3 );

	choose ( i ) {}
	choose ( i ) {
	  case 3:
		f( 3 );
	} // choose

	choose ( i ) {
		int j = 0;
		int k = 0;
		struct S { int i; };
		S s;
	  case 19:
	  case 'A'...'Z':
	  case 0 ...6:										// space required, or lexed as decimal point
	  case 20, 30, 40:
		i = 3;
		f( 3 );
	  default:
		j = 3;
	  case 8~10:
		f( 3 );
		fallthrough;
	  case 'd':
		j = 5;
	} // choose

	printf("done\n");
} // main

