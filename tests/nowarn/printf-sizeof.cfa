	for ( i; N ) {
        if (i > 0) printf(" ");
        printf( "%zu", i); // FIX ME: i still getting type long unsigned int on -m32
	}
    printf("\n");
  #endif
}

int main() {
    char c = 'x';
    fred(c);

    array( char, 10 ) a;
    mary(a);

    return 0;
}
