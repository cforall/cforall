	fred f1 = {3.14};
	fred f2 = {0.5};

	dlist(fred, fred.mine) lf;

	verify(validate(lf));

	printMyFreddies(f1, f2, 1);     // 3.14; 3.14; 0.5; 0.5
	printYourFreddies(f1, f2, 1);   // 3.14; 3.14; 0.5; 0.5

	insert_first(lf, f2);
	insert_first(lf, f1);

