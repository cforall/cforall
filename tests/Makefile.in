	find ${builddir} -not -path './__pycache__/*' -path '*/.err/*.log' -delete
	find ${builddir} -not -path './__pycache__/*' -path '*/.out/*.log' -delete
	rm -f ${EXTRA_PROGRAMS}
	rm -rf __pycache__

distclean-local :
	find ${builddir} -path '*.Po' -delete

