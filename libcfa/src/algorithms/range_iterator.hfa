generator RangeIter {
	const char * text;
	int com;
	int _start;
	int _stop;
};

static inline void ?{}(RangeIter & this, const char * text) {
	this.text = text;
}

static inline bool moveNext(RangeIter & this) { resume(this); return this.com >= 0; }